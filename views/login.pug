extends layout

block content
	h1 #{title}
	form(method='POST', action='/users/login')
		#form-group
			label User Name:
			input.form-control(type="email", name="email")
		#form-group
			label Password:
			input.form-control(type="password", name="password")
		br
		input.btn.btn-primary.my-3(type="submit",value="Submit")